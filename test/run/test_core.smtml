(let-const x bool)
(let-const y bool)
(let-const z bool)
(let-const w bool)
(assert (not (= x false)))
(assert
  (let ((not_x (not x)))
  (let ((not_y (not y)))
  (and (or not_x y) (or not_y z)))))
(assert (or (not x) z))
(check-sat)
(assert
  (let ((v (if (bool.xor x z) true false)))
  (= w v)))
(assert (or (not (and x z)) (not w)))
(check-sat)
(get-model)
