(let-const x str)
(let-const y real)
(let-const z real)
(push)
(assert
  (let ((str_y (real.to_string y)) (str_z (real.to_string z)))
  (bool.and (= y z) (= str_y str_z))))
(check-sat)
(pop 1)
(assert (= x "abc"))
(assert (= (str.len x) (str.len "abc")))
(assert (= "ab" (str.sub x 0 2)))
(assert (= (str.len (str.sub x 0 2)) 2))
(assert (= (str.to_code (str.nth x 0)) 97))
(assert (= (str.from_code (str.to_code (str.nth x 0))) "a"))
(check-sat)
(get-model)
