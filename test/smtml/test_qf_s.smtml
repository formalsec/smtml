(let-const x str)
(assert (= 4 (str.len x)))
(assert (str.prefixof "ab" x))
(assert (str.suffixof "cd" x))
(assert (str.contains x "bc"))
(assert (= "ab" (str.substr x 0 2)))
(assert (= "axyd" (str.replace x "bc" "xy")))
(assert (= 1 (str.indexof x "bc" 0)))
(assert (= 97 (str.to_code (str.at x 0))))
(assert (= "a" (str.from_code (str.to_code (str.at x 0)))))
(assert (= 42 (str.to_int "42")))
(assert (= "42" (str.from_int (str.to_int "42"))))
(check-sat)
(get-model)
